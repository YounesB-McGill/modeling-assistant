<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="modelingassistant" nsURI="http://cs.mcgill.ca/sel/modelingassistant/1.0"
    nsPrefix="modelingassistant">
  <eClassifiers xsi:type="ecore:EDataType" name="Time" instanceClassName="java.sql.Time"/>
  <eClassifiers xsi:type="ecore:EClass" name="ModelingAssistant">
    <eStructuralFeatures xsi:type="ecore:EReference" name="learningItems" upperBound="-1"
        eType="#//LearningItem" containment="true" eOpposite="#//LearningItem/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="learningResources" upperBound="-1"
        eType="#//LearningResource" containment="true" eOpposite="#//LearningResource/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="problemStatements" upperBound="-1"
        eType="#//ProblemStatement" containment="true" eOpposite="#//ProblemStatement/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="solutions" upperBound="-1"
        eType="#//Solution" containment="true" eOpposite="#//Solution/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="umlElements" upperBound="-1"
        eType="#//UmlElement" containment="true" eOpposite="#//UmlElement/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="students" upperBound="-1"
        eType="#//Student" containment="true" eOpposite="#//Student/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="feedbacks" upperBound="-1"
        eType="#//Feedback" containment="true" eOpposite="#//Feedback/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakeTypes" upperBound="-1"
        eType="#//MistakeType" containment="true" eOpposite="#//MistakeType/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="studentKnowledges" upperBound="-1"
        eType="#//StudentKnowledge" containment="true" eOpposite="#//StudentKnowledge/modelingAssistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakeTypeCategories" upperBound="-1"
        eType="#//MistakeTypeCategory" containment="true" eOpposite="#//MistakeTypeCategory/modelingAssistant"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Student" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/students"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="solutions" upperBound="-1"
        eType="#//Solution" eOpposite="#//Solution/student"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="studentKnowledges" upperBound="-1"
        eType="#//StudentKnowledge" eOpposite="#//StudentKnowledge/student"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="currentSolution" eType="#//Solution"
        eOpposite="#//Solution/currentStudent"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ProblemStatement" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="problemStatementElements"
        upperBound="-1" eType="#//ProblemStatementElement" containment="true" eOpposite="#//ProblemStatementElement/problemStatement"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="text" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/problemStatements"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="studentSolution" upperBound="-1"
        eType="#//Solution" eOpposite="#//Solution/studentProblemStatement"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="instructorSolution" lowerBound="1"
        eType="#//Solution" eOpposite="#//Solution/instructorProblemStatement"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ProblemStatementElement" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="problemStatement" lowerBound="1"
        eType="#//ProblemStatement" eOpposite="#//ProblemStatement/problemStatementElements"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="solutionElements" upperBound="-1"
        eType="#//SolutionElement" eOpposite="#//SolutionElement/problemStatementElements"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="UmlElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/umlElements"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="learningItems" upperBound="-1"
        eType="#//LearningItem" eOpposite="#//LearningItem/umlElements"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Solution">
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/solutions"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="student" eType="#//Student"
        eOpposite="#//Student/solutions"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="solutionElements" upperBound="-1"
        eType="#//SolutionElement" containment="true" eOpposite="#//SolutionElement/solution"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="classDiagram" lowerBound="1"
        eType="ecore:EClass classdiagram.ecore#//ClassDiagram"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakes" upperBound="-1"
        eType="#//Mistake" containment="true" eOpposite="#//Mistake/studentSolution"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="currentMistake" eType="#//Mistake"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="studentProblemStatement"
        lowerBound="1" eType="#//ProblemStatement" eOpposite="#//ProblemStatement/studentSolution"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="instructorProblemStatement"
        lowerBound="1" eType="#//ProblemStatement" eOpposite="#//ProblemStatement/instructorSolution"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="currentStudent" eType="#//Student"
        eOpposite="#//Student/currentSolution"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SolutionElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="problemStatementElements"
        upperBound="-1" eType="#//ProblemStatementElement" eOpposite="#//ProblemStatementElement/solutionElements"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="solution" lowerBound="1"
        eType="#//Solution" eOpposite="#//Solution/solutionElements"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="studentElementMistakes"
        upperBound="-1" eType="#//Mistake" eOpposite="#//Mistake/studentElements"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="element" lowerBound="1"
        eType="ecore:EClass classdiagram.ecore#//NamedElement"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="instructorElementMistakes"
        upperBound="-1" eType="#//Mistake" eOpposite="#//Mistake/instructorElements"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LearningItem" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/learningItems"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="umlElements" upperBound="-1"
        eType="#//UmlElement" eOpposite="#//UmlElement/learningItems"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="learningResources" upperBound="-1"
        eType="#//LearningResource" eOpposite="#//LearningResource/learningItem"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakeTypes" upperBound="-1"
        eType="#//MistakeType" eOpposite="#//MistakeType/learningItem"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StudentKnowledge">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="levelOfKnowledge" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="student" lowerBound="1"
        eType="#//Student" eOpposite="#//Student/studentKnowledges"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakeType" lowerBound="1"
        eType="#//MistakeType" eOpposite="#//MistakeType/studentKnowledges"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/studentKnowledges"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MistakeType" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="atomic" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timeToAddress" eType="#//Time"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numStepsBeforeNotification"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/mistakeTypes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="learningItem" lowerBound="1"
        eType="#//LearningItem" eOpposite="#//LearningItem/mistakeTypes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="studentKnowledges" upperBound="-1"
        eType="#//StudentKnowledge" eOpposite="#//StudentKnowledge/mistakeType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakes" upperBound="-1"
        eType="#//Mistake" eOpposite="#//Mistake/mistakeType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="feedbacks" upperBound="-1"
        eType="#//Feedback" eOpposite="#//Feedback/mistakeType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakeTypeCategory" lowerBound="1"
        eType="#//MistakeTypeCategory" eOpposite="#//MistakeTypeCategory/mistakeTypes"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Mistake">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="resolved" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timeToAddress" eType="#//Time"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numStepsBeforeNotification"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="studentElements" upperBound="-1"
        eType="#//SolutionElement" eOpposite="#//SolutionElement/studentElementMistakes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakeType" lowerBound="1"
        eType="#//MistakeType" eOpposite="#//MistakeType/mistakes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="lastFeedback" eType="#//Feedback"
        eOpposite="#//Feedback/mistakes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="instructorElements" upperBound="-1"
        eType="#//SolutionElement" eOpposite="#//SolutionElement/instructorElementMistakes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="studentSolution" lowerBound="1"
        eType="#//Solution" eOpposite="#//Solution/mistakes"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numDetection" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numDetectionSinceResolved"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Feedback">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="level" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="congratulatory" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="usefulness" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="highlightProblem" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="highlightSolution" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/feedbacks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakeType" lowerBound="1"
        eType="#//MistakeType" eOpposite="#//MistakeType/feedbacks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakes" upperBound="-1"
        eType="#//Mistake" eOpposite="#//Mistake/lastFeedback"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="text" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TextResponse" eSuperTypes="#//Feedback"/>
  <eClassifiers xsi:type="ecore:EClass" name="ParametrizedResponse" eSuperTypes="#//Feedback"/>
  <eClassifiers xsi:type="ecore:EClass" name="ResourceResponse" eSuperTypes="#//Feedback">
    <eStructuralFeatures xsi:type="ecore:EReference" name="learningResources" lowerBound="1"
        upperBound="-1" eType="#//LearningResource" eOpposite="#//LearningResource/resourceResponses"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LearningResource" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/learningResources"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="learningItem" lowerBound="1"
        eType="#//LearningItem" eOpposite="#//LearningItem/learningResources"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="resourceResponses" upperBound="-1"
        eType="#//ResourceResponse" eOpposite="#//ResourceResponse/learningResources"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="content" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaObject"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Reference" eSuperTypes="#//LearningResource"/>
  <eClassifiers xsi:type="ecore:EClass" name="Tutorial" eSuperTypes="#//LearningResource"/>
  <eClassifiers xsi:type="ecore:EClass" name="Example" eSuperTypes="#//LearningResource"/>
  <eClassifiers xsi:type="ecore:EClass" name="Quiz" eSuperTypes="#//LearningResource"/>
  <eClassifiers xsi:type="ecore:EClass" name="NamedElement" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MistakeTypeCategory" eSuperTypes="#//NamedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="mistakeTypes" upperBound="-1"
        eType="#//MistakeType" eOpposite="#//MistakeType/mistakeTypeCategory"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="supercategory" eType="#//MistakeTypeCategory"
        eOpposite="#//MistakeTypeCategory/subcategories"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subcategories" upperBound="-1"
        eType="#//MistakeTypeCategory" eOpposite="#//MistakeTypeCategory/supercategory"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingAssistant" lowerBound="1"
        eType="#//ModelingAssistant" eOpposite="#//ModelingAssistant/mistakeTypeCategories"/>
  </eClassifiers>
</ecore:EPackage>
